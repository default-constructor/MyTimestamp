<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/imageViewFragmentProjekteTitelIconLeft"
            android:layout_alignParentStart="true"
            android:layout_width="32dp"
            android:layout_height="match_parent"/>
        <TextView
            android:id="@+id/textViewFragmentProjekteTitelText"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textAllCaps="true"
            android:textSize="16sp"
            android:textStyle="bold"/>
        <ImageView
            android:id="@+id/imageViewFragmentProjekteTitelIconRight"
            android:layout_alignParentEnd="true"
            android:layout_width="32dp"
            android:layout_height="match_parent"/>
    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/linearLayoutFragmentProjekteWrapperProjekte"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        </LinearLayout>
    </ScrollView>
</LinearLayout>