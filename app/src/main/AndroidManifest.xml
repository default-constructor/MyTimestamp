<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.defaultconstructor.mytimestamp">
    <uses-sdk android:minSdkVersion="17" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name="de.defaultconstructor.mytimestamp.app.App"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="de.defaultconstructor.mytimestamp.app.android.activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.defaultconstructor.mytimestamp.app.android.activities.SettingsActivity"
            android:label="Einstellungen"
            android:parentActivityName="de.defaultconstructor.mytimestamp.app.android.activities.MainActivity"
            android:windowSoftInputMode="stateHidden"/>
    </application>
</manifest>
